model Ingredient {
  id String @id @default(cuid())

  name    String
  iconUrl String  @map("icon_url")
  content String
  price   Decimal

  recipesIngridient RecipeIngredient[]

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("ingredients")
}

enum Unit {
  GRAM // г
  MILLILITER // мл
  PIECE // шт
  TEASPOON // ч.л.
  TABLESPOON // ст.л.
  CLOVES // зубчик
}
